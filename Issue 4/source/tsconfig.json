{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "es6",
    "outDir": "../dist",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
